angular.module('produto')
.service(
	'produtoService',
	[	
	 	'$http',
	 	function produtoService($http) {
	 		
	 		var url = "http://localhost:8080/aula-java-rest/rest/produto/";
	 		
	 		produtoService.findAll = function() {
	 			return $http.get(url + 'find-all')
	 			.
	 		}
	 		
	 		return produtoService;
	 	}
	]	
);
